{
    "name": "Flipkart Order Analyzer",
    "description": "Flipkart Order Analyzer.",
    "version": "1.1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./index.html",
        "default_icon": "./assets/logo.png"
    },
    "icons" : {
        "48": "./assets/logo.png",
        "128": "./assets/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./contentScript.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}